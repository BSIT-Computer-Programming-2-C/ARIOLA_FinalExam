class Person:
    def __init__(self, name, age, address):
        self.__name = name
        self.__age = age
        self.__address = address

    def get_name(self):
        return self.__name

    def set_name(self, name):
        self.__name = name

    def get_age(self):
        return self.__age

    def set_age(self, age):
        self.__age = age

    def get_address(self):
        return self.__address

    def set_address(self, address):
        self.__address = address

person = Person("Jhon Rey", 19, "Lower Quirino Hill Purok 3 #68")
print(person.get_name())
person.set_age(19)
print(person.get_age())
person.set_address("Lower Quirino Hill Purok 3 #68")
print(person.get_address())
